QVariant ParticipantsModel::data(const QModelIndex &idx, int Role) const
{
    if(!idx.isValid())
        return QVariant();
    return (Role == Qt::DisplayRole || Role == Qt::EditRole) ?
                QVariant::fromValue(m_pList.at(idx.row())) : QVariant();
}

